<Window x:Class="AssistantPaladin.OverlayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssistantPaladin"
        mc:Ignorable="d"
        Title="OverlayWindow" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <Ellipse Name="ellipseAim" Opacity="0.9" Height="30" Width="30" Stroke="Gray" StrokeThickness="3"></Ellipse>
    </Grid>
</Window>